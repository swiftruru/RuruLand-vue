{
  "name": "portfolio-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Modern full-stack developer portfolio built with Vue 3 Composition API, TypeScript, and Vite. Features i18n support, accessibility, Google Analytics integration, and comprehensive test coverage (333 tests, 86.57% coverage).",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite-ssg build",
    "type-check": "vue-tsc --build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "pinia": "^3.0.4",
    "vue": "^3.5.26",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^11.0.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node24": "^24.0.3",
    "@types/node": "^24.10.4",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "@vueuse/head": "^2.0.0",
    "gh-pages": "^6.3.0",
    "happy-dom": "^20.1.0",
    "jsdom": "^27.0.1",
    "npm-run-all2": "^8.0.4",
    "typescript": "~5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vite-ssg": "^28.2.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.2.2",
    "workbox-window": "^7.4.0"
  }
}
